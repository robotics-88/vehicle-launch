<launch>
    <arg name="camera_name" default="see3cam"/>
    <arg name="camera_param_yaml" default="$(find-pkg-share vehicle_launch)/config/$(var camera_name)_params.yaml"/>

    <group>
        <push-ros-namespace namespace="$(var camera_name)"/>

        <node pkg="usb_cam" exec="usb_cam_node_exe" output="log">
            <param from="$(var camera_param_yaml)"/>
        </node>
    </group>

    
</launch>